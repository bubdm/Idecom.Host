<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Idecom.Host</id>
    <version>1.1.15</version>
    <authors>Evgeny Komarevtsev</authors>
    <description>Idecom AnyCPU host</description>
    <releaseNotes>Saving project after adding start program so multiple packages could be installed in a single NuGet transaction.</releaseNotes>
    <dependencies>
      <dependency id="Topshelf" version="[3.1.3]" />
    </dependencies>
  </metadata>
 <files>
    <file src="bin\debug\Idecom.Host.exe" target="lib" />
    <file src="bin\debug\Idecom.Host.pdb" target="lib" />
    <file src="tools\*.*" target="tools" />
    <file src="**\*.cs" target="src" exclude="bin\**;obj\**;tools\**" />
  </files>
</package>